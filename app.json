{
  "expo": {
    "name": "Apexnews",
    "slug": "apexnews",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/apexnews.png",
    "userInterfaceStyle": "dark",
    "scheme": "apexnews",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/apexnews.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"]
  }
}
